<script>
  window.addEventListener("load", function(event) {
    var MAX_RETRY_TIME = 5;
    var INTERVAL_TO_RETRY = 10000;
  
    function tryToSetIdentity() {
      if ( typeof tryToSetIdentity.counter == 'undefined' ) {
          tryToSetIdentity.counter = 0;
      }
  
      if (tryToSetIdentity.counter++ > MAX_RETRY_TIME) {
          return;
      }
  
      if (!window.zE) {
          setTimeout(tryToSetIdentity, INTERVAL_TO_RETRY);
          return;
      }
  
      window.zE(function() {
        window.zE.identify({
          name: "Vitruves",
          email: "johan.natter@laposte.net"
        });
      });
    }
  
    tryToSetIdentity()
  });
</script>


